package Model;

public enum Difficulty {
	
	EASY(1);
	
	private int width;
	private int height;
	private int sizeOfCells;
	
	private Difficulty(int level){
		if(level == 1){
			setWidth(level);
		}
	}

	public int getWidth() {
		return width;
	}

	public void setWidth(int width) {
		this.width = width;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}
	
	
}
